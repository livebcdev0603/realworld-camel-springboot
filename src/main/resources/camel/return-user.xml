<routes xmlns="http://camel.apache.org/schema/spring">

  <route id="direct_returnUser" routeConfigurationId="return-cleanup">
    <from uri="direct:returnUser" />

    <to uri="direct:prepareReturnUser" />

    <marshal><json /></marshal>
    <to uri="jslt:jslt/user-resp.jslt" />

    <unmarshal>
      <json unmarshalType="com.github.jacekszymanski.realcamel.model.Login200Response"
            disableFeatures="FAIL_ON_UNKNOWN_PROPERTIES" />
    </unmarshal>

  </route>

</routes>
